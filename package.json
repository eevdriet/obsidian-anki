		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.8",
		"@types/crypto-js": "^4.2.2",
		"@types/micromatch": "^4.0.9",
		"@types/showdown": "^2.0.6",
		"crypto-js": "^4.2.0",
		"micromatch": "^4.0.8",
